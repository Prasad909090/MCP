{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "airbnb": {
      "command": "npx",
      "args": [
        "-y",
        "@openbnb/mcp-server-airbnb"
      ]
    },
    "ddg-search": {
      "command": "uvx",
      "args": ["duckduckgo-mcp-server"
      ]
    },
    "excel": {
      "command": "cmd",
      "args": ["/c", "npx", "--yes", "@negokaz/excel-mcp-server"
    ],
    "env": {
      "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
    }
    }
  }
}

